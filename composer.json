{
    "name":"newage/console-tools",
    "description": "Zend Framework 2 console module for work with database",
    "keywords": [
        "zf2",
        "tool",
        "console"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/newage/console-tools.git"
        }
    ],
    "homepage": "https://github.com/newage/console-tools.git",
    "license": "MIT",
    "require":{
        "php":">=5.3.3",
        "zendframework/zendframework": ">=2.2.0"
    },
    "require-dev":{
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "1.5.*"
    },
    "autoload":{
        "psr-0":{
            "ConsoleTools":"src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
